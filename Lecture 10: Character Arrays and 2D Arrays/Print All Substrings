


void printSubstrings(char input[]) {
    // Write your code here
    for(int k=0;input[k]!='\0';k++){
    	for(int i=0;input[i]!='\0';i++){
    		bool a=false;
        	for(int j=k;j<=i;j++){
            	cout<<input[j];
            	a=true;
    	    }
    	    if(a==true){
    	    cout<<endl;
			}
   		}
	}
}



